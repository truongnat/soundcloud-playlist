{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build:simple": "cross-env NITRO_PRESET=node-server nuxt build",
    "build:fast": "node scripts/build-optimize.js",
    "build:analyze": "NUXT_ANALYZE=true nuxt build",
    "build:fix": "npm run clean:all && cross-env NODE_OPTIONS=--max-old-space-size=8192 NITRO_PRESET=node-server nuxt build",
    "dev": "nuxt dev",
    "dev:fast": "nuxt dev --no-fork",
    "dev:clean": "npm run clean && nuxt dev",
    "dev:debug": "nuxt dev --inspect",
    "dev:host": "nuxt dev --host 0.0.0.0",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "clean": "rm -rf .nuxt .output dist node_modules/.cache",
    "clean:all": "rm -rf .nuxt .output dist node_modules/.cache node_modules/.vite",
    "fix-dev": "node scripts/fix-dev-server.js",
    "typecheck": "nuxt typecheck",
    "build:monitor": "node scripts/build-monitor.js",
    "test:build": "node scripts/test-build-performance.js"
  },
  "dependencies": {
    "@ffmpeg/core": "^0.12.10",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@headlessui/vue": "^1.7.23",
    "@nuxt/ui": "^3.1.3",
    "@nuxtjs/sitemap": "^7.4.1",
    "@nuxtjs/tailwindcss": "^7.0.0-beta.0",
    "@pinia/nuxt": "0.11.1",
    "@types/howler": "^2.2.12",
    "axios": "^1.9.0",
    "howler": "^2.2.4",
    "m3u8-parser": "^7.2.0",
    "nuxt": "^3.17.5",
    "pinia": "^3.0.3",
    "scrape-it": "^6.1.5",
    "soundcloud-api-client": "^0.0.9",
    "soundcloud-scraper": "^5.0.3",
    "soundcloud-v2-api": "^0.1.4",
    "soundcloud.ts": "^0.6.5",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/m3u8-parser": "^7.2.2",
    "cross-env": "^7.0.3"
  }
}